<!DOCTYPE html>
<html>
<head>
    <% include ../partials/header.ejs %>
    <link rel="stylesheet" type="text/css" href="/stylesheets/registration.css"/>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="row" id="page">
    <div id="sidebar-wrapper" class="col-md-2">
        <div id="sidebar">
            <h3>Existing Kids:</h3>
            <ul>
                <li>
                    Example Child
                </li>
            </ul>
        </div>
    </div>
    <div class="col-md-8 col-md-offset-2">
        <br><br>
        <div class="form-group col-md-1">
            <img src="/images/pig_filled_add.gif">
        </div>

        <div class="form-group col-md-11">
            <h2>Add A Child Account</h2>
        </div>

        <!-- Line Break -->
        <div class="form-group col-md-12"></div>

        <div class="form-group col-md-8">
            <label>Child Name</label>
            <input type="" name="" class="form-control" id="" value="">
        </div>

        <!-- Line Break -->
        <div class="form-group col-md-12"></div>

        <div class="form-group col-md-8">
            <label>Username</label>
            <input type="" name="" class="form-control" id="" value="">
        </div>

        <!-- Line Break -->
        <div class="form-group col-md-12"></div>

        <div class="form-group col-md-8">
            <label>Password</label>
            <input type="password" name="" class="form-control" id="" value="">
        </div>

        <!-- Line Break -->
        <div class="form-group col-md-12"></div>

        <div class="form-group col-md-8">
            <label>Confirm Password</label>
            <input type="password" name="" class="form-control" id="" value="">
        </div>

        <!-- Line Break -->
        <div class="form-group col-md-12"><br></div>

        <div class="form-group col-md-4">
            <button type="Cancel" class="btn btn-primary" id="cancel">Cancel</button>
        </div>

        <div class="form-group col-md-4">
            <button type="Submit" class="btn btn-primary" id="submit">Submit</button>
        </div>

    </div>
</div>

</body>
</html>